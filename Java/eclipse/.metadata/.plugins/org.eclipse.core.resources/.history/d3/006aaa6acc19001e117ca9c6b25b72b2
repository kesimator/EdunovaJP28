package edunova;

public class Grad {
	
	private String naziv;
	
	private int stanovnika;
	
	private boolean crkvaKatolicka;
	
	private char energetskiCertifikat;
	
	private double bdp;

	public String getNaziv() {
		return naziv;
	}

	public void setNaziv(String naziv) {
		this.naziv = naziv;
	}

	public int getStanovnika() {
		return stanovnika;
	}

	public void setStanovnika(int stanovnika) {
		this.stanovnika = stanovnika;
	}

	public boolean isCrkvaKatolicka() {
		return crkvaKatolicka;
	}

	public void setCrkvaKatolicka(boolean crkvaKatolicka) {
		this.crkvaKatolicka = crkvaKatolicka;
	}

	public char getEnergetskiCertifikat() {
		return energetskiCertifikat;
	}

	public void setEnergetskiCertifikat(char energetskiCertifikat) {
		this.energetskiCertifikat = energetskiCertifikat;
	}

	public double getBdp() {
		return bdp;
	}

	public void setBdp(double bdp) {
		this.bdp = bdp;
	}

	
	

}
